<div class="modal fade" id="CateModal" tabindex="-1" aria-labelledby="CateModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h1 class="modal-title fs-5" id="CateModalLabel">Expence Type Entry</h1>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                <form>
                                    <div class="row">
                                        <input id="id" name="id" type="text" class="form-control" hidden>
                                        <div class="mb-6">
                                            <label for="ex_name">Type Name</label>
                                            <input id="ex_name" name="ex_name" type="text" class="form-control" placeholder="Type Name">
                                        </div>
                                        <div class="mb-6 mt-5">
                                            <div class="form-check form-check-success mb-3">

                                                <label class="form-check-label" for="is_active">
                                                    Is Active?
                                                </label>
                                                <input class="form-check-input" type="checkbox" name="is_active" id="is_active" checked>
                                            </div>
                                        </div>
                                    </div>
                                </form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" id="savebutton" onclick="Submit()">Save Changes</button>
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>

            </div>
        </div>
    </div>
</div>




